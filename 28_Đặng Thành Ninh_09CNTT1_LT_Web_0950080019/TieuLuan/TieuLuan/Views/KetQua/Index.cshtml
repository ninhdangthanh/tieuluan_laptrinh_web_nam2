@model IEnumerable<TieuLuan.Models.KetQua>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Bảng Kết Quả</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã sinh viên
        </th>
        <th>
            Mã khóa học
        </th>
        <th>
            Lần thi
        </th>
        <th>
            Điểm
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaSinhVien)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaKhoaHoc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LanThi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Diem)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id = item.MaSinhVien }) |
            @Html.ActionLink("Chi tiết", "Details", new { id = item.MaSinhVien }) |
            @Html.ActionLink("Xóa", "Delete", new { id = item.MaSinhVien })
        </td>
    </tr>
}

</table>
