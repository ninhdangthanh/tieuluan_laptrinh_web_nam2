@model IEnumerable<TL_LT_Web_14.Models.NhanKhau>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Nhân khẩu</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Mã nhân khẩu
        </th>
        <th>
            Tên nhân khẩu
        </th>
        <th>
            Phái nữ
        </th>
        <th>
            Ngày sinh
        </th>
        <th>
            Số CMND
        </th>
        <th>
            Mã trình độ văn hóa
        </th>
        <th>
           Mã ngoại ngữ
        </th>
        <th>
            Số sổ hộ khẩu
        </th>
        <th>
           Ghi chú
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaNhanKhau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenNhanKhau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhaiNu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgaySinh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SoCMND)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaTrinhDoVanHoa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaNgoaiNgu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SoSoHoKhau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GhiChu)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id = item.MaNhanKhau }) |
            @Html.ActionLink("Chi tiết", "Details", new { id = item.MaNhanKhau }) |
            @Html.ActionLink("Xóa", "Delete", new { id = item.MaNhanKhau })
        </td>
    </tr>
}

</table>
