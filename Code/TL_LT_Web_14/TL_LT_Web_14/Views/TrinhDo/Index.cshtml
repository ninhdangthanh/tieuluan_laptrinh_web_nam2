@model IEnumerable<TL_LT_Web_14.Models.TrinhDo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Trình độ</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã trình độ văn hóa
        </th>
        <th>
           Tên trình độ văn hóa
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaTrinhDoVanHoa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenTrinhDoVanHoa)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id = item.MaTrinhDoVanHoa }) |
            @Html.ActionLink("Chi tiết", "Details", new { id = item.MaTrinhDoVanHoa }) |
            @Html.ActionLink("Xóa", "Delete", new { id = item.MaTrinhDoVanHoa })
        </td>
    </tr>
}

</table>
